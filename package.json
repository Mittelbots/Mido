{
  "name": "mido",
  "version": "0.13.2",
  "description": "Mido. Manage your tasks like a pro",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "prod": "pm2 start shard.js --name Mido --max-memory-restart 500M && pm2 monit",
    "restart": "pm2 stop Mido && pm2 start shard.js --name Mido --max-memory-restart 500M && pm2 monit",
    "kill": "pm2 stop Mido && pm2 delete Mido",
    "mido_install": "mkdir _logs && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mittelbots/Mido.git"
  },
  "author": "BlackDayz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mittelbots/Mido/issues"
  },
  "homepage": "https://github.com/Mittelbots/Mido#readme",
  "dependencies": {
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "discord-api-types": "^0.34.0",
    "discord.js": "^13.8.0",
    "mysql": "^2.18.1",
    "node-sloc": "^0.2.1",
    "nodemon": "^2.0.16",
    "pm2": "^5.2.0",
    "randomcolor": "^0.6.2",
    "simple-node-logger": "^21.8.12",
    "sloc": "^0.2.1"
  }
}
