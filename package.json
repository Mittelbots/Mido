{
    "name": "mido",
    "version": "0.14.1",
    "description": "Mido. Manage your tasks like a pro",
    "main": "index.js",
    "scripts": {
        "start": "docker compose up -d && docker logs -f mido",
        "stop": "docker compose down",
        "restart": "npm run stop && npm run start",
        "mido_install": "mkdir _logs && npm i",
        "format": "prettier --write . --bracket-same-line=true",
        "check": "prettier --check . --bracket-same-line=true"
    },
