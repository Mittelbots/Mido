    "bugs": {
        "url": "https://github.com/Mittelbots/Mido/issues"
    },
    "homepage": "https://github.com/Mittelbots/Mido#readme",
    "dependencies": {
        "@discordjs/rest": "^1.0.1",
        "discord.js": "^14.11.0",
        "dotenv": "^16.0.3",
        "mysql": "^2.18.1",
        "node-sloc": "^0.2.1",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "randomcolor": "^0.6.2",
        "simple-node-logger": "^21.8.12",
        "sloc": "^0.2.1"
    }
